<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@mipmap/loginandreg">
    <!--注册页面-->
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@null"
        android:layout_marginBottom="@dimen/dp_10"
        android:layout_marginLeft="@dimen/dp_80"
        app:layout_constraintBottom_toTopOf="@id/view1"
        android:layout_marginRight="@dimen/dp_45"
        android:hint="昵称"
        android:textStyle="bold"
        android:textColor="#c61573"
        android:textSize="@dimen/sp_18"
        android:textColorHint="#ffffff"
        android:id="@+id/register_ed_nickname"
        />

    <ImageView
        android:layout_width="@dimen/dp_25"
        android:layout_height="@dimen/dp_25"
        android:src="@mipmap/register_icon_name_default_xhdpi"
        app:layout_constraintBottom_toTopOf="@id/view1"
        app:layout_constraintLeft_toLeftOf="@id/view1"
        android:layout_marginBottom="@dimen/dp_10"
        />
    <View
        android:id="@+id/view1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_marginRight="@dimen/dp_45"
        android:layout_height="@dimen/dp_1"
        android:layout_marginLeft="@dimen/dp_45"
        android:background="#e2e1e1"
        android:layout_marginTop="@dimen/dp_200"
        />
    <EditText
        android:textStyle="bold"
        android:textColor="#c61573"
        android:layout_marginTop="@dimen/dp_230"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@null"
        android:layout_marginLeft="@dimen/dp_80"
        android:layout_marginRight="@dimen/dp_45"
        android:hint="性别"
        app:layout_constraintBottom_toTopOf="@id/view2"
        android:layout_marginBottom="@dimen/dp_10"
        android:textSize="@dimen/sp_18"
        android:textColorHint="#ffffff"
        android:id="@+id/register_ed_sex"
        />
    <ImageView
        app:layout_constraintBottom_toTopOf="@id/view2"
        android:layout_marginBottom="@dimen/dp_10"
        app:layout_constraintLeft_toLeftOf="@id/view2"
        android:layout_width="@dimen/dp_26"
        android:layout_height="@dimen/dp_26"
        android:layout_marginTop="@dimen/dp_230"
        android:src="@mipmap/register_icon_sex_default_xxhdpi"
        />
    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginLeft="@dimen/dp_45"
        android:background="#e2e1e1"
        android:layout_marginRight="@dimen/dp_45"
        android:layout_marginTop="@dimen/dp_70"
        app:layout_constraintTop_toBottomOf="@id/view1"
        />
    <EditText
        android:textStyle="bold"
        android:textColor="#c61573"
        android:layout_marginTop="@dimen/dp_230"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@null"
        android:layout_marginLeft="@dimen/dp_80"
        android:layout_marginRight="@dimen/dp_45"
        android:hint="出生日期"
        app:layout_constraintBottom_toTopOf="@id/view3"
        android:layout_marginBottom="@dimen/dp_10"
        android:textSize="@dimen/sp_18"
        android:textColorHint="#ffffff"
        android:id="@+id/register_ed_date"
        />
    <ImageView
        app:layout_constraintBottom_toTopOf="@id/view3"
        android:layout_marginBottom="@dimen/dp_10"
        app:layout_constraintLeft_toLeftOf="@id/view3"
        android:layout_width="@dimen/dp_26"
        android:layout_height="@dimen/dp_26"
        android:layout_marginTop="@dimen/dp_230"
        android:src="@mipmap/register_icon_date_default_xxhdpi"
        />
    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginLeft="@dimen/dp_45"
        android:background="#e2e1e1"
        android:layout_marginRight="@dimen/dp_45"
        android:layout_marginTop="@dimen/dp_70"
        app:layout_constraintTop_toBottomOf="@id/view2"
        />
    <EditText
        android:textStyle="bold"
        android:textColor="#c61573"
    android:layout_marginTop="@dimen/dp_230"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@null"
    android:layout_marginLeft="@dimen/dp_80"
    android:layout_marginRight="@dimen/dp_45"
    android:hint="手机号"
    app:layout_constraintBottom_toTopOf="@id/view4"
    android:layout_marginBottom="@dimen/dp_10"
    android:textSize="@dimen/sp_18"
    android:textColorHint="#ffffff"
    android:id="@+id/register_ed_phone"
    />
    <ImageView
        app:layout_constraintBottom_toTopOf="@id/view4"
        android:layout_marginBottom="@dimen/dp_10"
        app:layout_constraintLeft_toLeftOf="@id/view4"
        android:layout_width="@dimen/dp_26"
        android:layout_height="@dimen/dp_26"
        android:layout_marginTop="@dimen/dp_230"
        android:src="@mipmap/register_icon_phone_default_xhdpi"
        />
    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginLeft="@dimen/dp_45"
        android:background="#e2e1e1"
        android:layout_marginRight="@dimen/dp_45"
        android:layout_marginTop="@dimen/dp_70"
        app:layout_constraintTop_toBottomOf="@id/view3"
        />
    <EditText
        android:textStyle="bold"
        android:textColor="#c61573"
        android:layout_marginTop="@dimen/dp_230"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@null"
        android:layout_marginLeft="@dimen/dp_80"
        android:layout_marginRight="@dimen/dp_45"
        android:hint="邮箱"
        app:layout_constraintBottom_toTopOf="@id/view5"
        android:layout_marginBottom="@dimen/dp_10"
        android:textSize="@dimen/sp_18"
        android:textColorHint="#ffffff"
        android:id="@+id/register_ed_email"
        />
    <ImageView
        app:layout_constraintBottom_toTopOf="@id/view5"
        android:layout_marginBottom="@dimen/dp_10"
        app:layout_constraintLeft_toLeftOf="@id/view5"
        android:layout_width="@dimen/dp_26"
        android:layout_height="@dimen/dp_26"
        android:layout_marginTop="@dimen/dp_230"
        android:src="@mipmap/register_icon_mail_default_xxhdpi"
        />
    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginLeft="@dimen/dp_45"
        android:background="#e2e1e1"
        android:layout_marginRight="@dimen/dp_45"
        android:layout_marginTop="@dimen/dp_70"
        app:layout_constraintTop_toBottomOf="@id/view4"
        />
    <EditText

        android:textStyle="bold"
        android:textColor="#c61573"
        android:layout_marginTop="@dimen/dp_230"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@null"
        android:layout_marginLeft="@dimen/dp_80"
        android:layout_marginRight="@dimen/dp_45"
        android:hint="登录密码"
        android:password="true"
        app:layout_constraintBottom_toTopOf="@id/view6"
        android:layout_marginBottom="@dimen/dp_10"
        android:textSize="@dimen/sp_18"
        android:textColorHint="#ffffff"
        android:id="@+id/register_ed_pwd"
        />
    <ImageView
        app:layout_constraintBottom_toTopOf="@id/view6"
        android:layout_marginBottom="@dimen/dp_10"
        app:layout_constraintLeft_toLeftOf="@id/view6"
        android:layout_width="@dimen/dp_26"
        android:layout_height="@dimen/dp_26"
        android:layout_marginTop="@dimen/dp_230"
        android:src="@mipmap/log_icon_lock_default_xxhdpi"
        />
    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginLeft="@dimen/dp_45"
        android:background="#e2e1e1"
        android:layout_marginRight="@dimen/dp_45"
        android:layout_marginTop="@dimen/dp_70"
        app:layout_constraintTop_toBottomOf="@id/view5"
        />
    <Button
        android:id="@+id/register_btn_register"
        android:textColor="#ffffff"
        android:textSize="@dimen/sp_18"
        android:background="@drawable/shape_login"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:layout_marginLeft="@dimen/dp_45"
        android:layout_marginRight="@dimen/dp_45"
        android:text="注册"
        app:layout_constraintTop_toBottomOf="@id/view6"
        android:layout_marginTop="@dimen/dp_120"
        />
</android.support.constraint.ConstraintLayout>